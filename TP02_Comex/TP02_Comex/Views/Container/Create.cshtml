@model CONTAINER

<h2>Cadastro de Container</h2>
<form asp-action="Create" method="post">
    <div class="form-group">
        <label>Número Container</label>
        <input asp-for="Numero" class="form-control" />
    </div>
    <div class="form-group">
        <label>Tipo</label>
        <input asp-for="CONTAINER_TYPE" class="form-control" />
    </div>
    <div class="form-group">
        <label>BL Associado</label>
        <select asp-for="Bl" asp-items="@(new SelectList(ViewBag.BLs, "Id", "NumeroBL"))" class="form-control"></select>
    </div>
    <button type="submit" class="btn btn-success">Salvar</button>
</form>
